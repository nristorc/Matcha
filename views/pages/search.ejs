<%- include('../partial/head')%>
<%- include('../partial/header')%>

<div class="container2">


</div>


<p id="loading">
		<img style="width:40px; margin-left:50%" src="https://ubisafe.org/images/transparent-gif-loading-1.gif" alt="Loadingâ€¦" />
	</p>





<script>
	var i =0;

	$(document).ready(function() {
		var win = $(window);
		win.scroll(function() {
			if ($(document).height() - win.height() == win.scrollTop()) {
				$('#loading').show();
				$.ajax({
					method: "GET",
					data: {
						index:i
					},
					url: 'search-infinite',
					success: function(html) {
						$('.container2').append(html);
						$('#loading').hide();
					}
				});
				i++;
			}
		});
	});
</script>
		




